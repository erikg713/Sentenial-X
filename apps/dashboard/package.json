{
  "name": "sentenialx-dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "Frontend dashboard for Sentenial-X cybersecurity platform",
  "author": "Sentenial-X Team",
  "license": "Proprietary",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write ."
  },
  "dependencies": {
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "axios": "^1.6.0",
    "classnames": "^2.3.2",
    "zustand": "^4.4.0",
    "dayjs": "^1.12.7"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "vite": "^5.0.0",
    "tailwindcss": "^4.0.0",
    "postcss": "^9.0.0",
    "autoprefixer": "^11.0.0",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.0.0",
    "typescript": "^5.2.0"
  },
  "eslintConfig": {
    "extends": ["react-app", "eslint:recommended", "plugin:react/recommended", "prettier"],
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": 13,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "es5"
  }
}
