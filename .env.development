# Flask
FLASK_ENV=development
FLASK_DEBUG=1
FLASK_APP=sentinel_core.py

# Logging
LOG_LEVEL=DEBUG
LOG_PATH=analytics/memory_scan_logs/emulation.log

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/sentenialx_db

# Redis
REDIS_URL=redis://localhost:6379/0

# Threat Intelligence
THREAT_INTEL_API_KEY=your_api_key_here
EXFILTRATION_ENDPOINT=https://omega-clouddrop.biz/data

# Emulation
POWERSHELL_EXECUTABLE=pwsh

# Email
SMTP_SERVER=smtp.mailtrap.io
SMTP_PORT=2525
SMTP_USERNAME=your_username
SMTP_PASSWORD=your_password

# Secrets
SECRET_KEY=your_flask_secret_key
JWT_SECRET=your_jwt_secret

# Features
ENABLE_DEEP_EMULATION=true
ENABLE_GUI_DASHBOARD=true